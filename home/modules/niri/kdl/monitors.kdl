



//Monitors ---- foreløpig kommneterr jeg ut de som ikke er i bruk



output "HDMI-A-1" {
  mode "3840x2160@60.000"
  scale 1.25
  position x=0 y=0
}

output "eDP-1" {
  mode "1920x1080@60.000"
  scale 1
  // ----laptop sentrert under ekstern skjerm
  //  position x=320 y=1440
  //---- lik øverste kant, laptop til venstre for ekstern skjerm
  position x=-1920 y=0
}
